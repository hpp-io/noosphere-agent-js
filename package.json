{
  "name": "noosphere-agent-js",
  "version": "0.1.0",
  "description": "Next.js-based Noosphere agent using noosphere-sdk",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "agent": "tsx src/app.ts",
    "init": "tsx scripts/init-keystore.ts",
    "setup:wallet": "tsx scripts/setup-agent-wallet.ts",
    "send:request": "tsx scripts/send-request.ts",
    "db:reset": "tsx scripts/db/reset-checkpoint.ts",
    "db:audit": "tsx scripts/db/audit-events.ts",
    "db:sync": "tsx scripts/db/sync-from-blockchain.ts",
    "generate:config": "tsx scripts/generate-config.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "docker:build": "docker compose -f docker/docker-compose.yml build",
    "docker:up": "docker compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker compose -f docker/docker-compose.yml down",
    "docker:logs": "docker compose -f docker/docker-compose.yml logs -f",
    "docker:restart": "docker compose -f docker/docker-compose.yml restart"
  },
  "dependencies": {
    "@noosphere/agent-core": "^0.1.0-alpha.13",
    "@noosphere/contracts": "^0.1.0-alpha.5",
    "@noosphere/crypto": "^0.1.0-alpha.5",
    "@noosphere/registry": "^0.1.0-alpha.7",
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "axios": "^1.13.2",
    "better-sqlite3": "^11.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "ethers": "^6.13.0",
    "express": "^5.2.1",
    "form-data": "^4.0.5",
    "next": "15.5.9",
    "postcss": "^8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "socket.io": "^4.8.3",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.1.1",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  }
}
